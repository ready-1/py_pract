#!/Users/bob/dev/Environments/py_pract/bin/python3.8
# EASY-INSTALL-ENTRY-SCRIPT: 'ppi==0.0.4','console_scripts','ppi'
__requires__ = 'ppi==0.0.4'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('ppi==0.0.4', 'console_scripts', 'ppi')()
    )
